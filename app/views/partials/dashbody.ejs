<div class="main-panel">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
        <div class="container-fluid">
            <div class="navbar-wrapper">
                <div class="navbar-toggle">
                    <button type="button" class="navbar-toggler">
                        <span class="navbar-toggler-bar bar1"></span>
                    </button>
                </div>
                <a class="navbar-brand" href="#">ADMINISTRADOR</a>
            </div>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation"
                aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-bar navbar-kebab"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navigation">
                <ul class="navbar-nav">
                    <li class="nav-item btn-rotate dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false">
                            <i class="fa fa-cog" aria-hidden="true"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item" href="../dash/exit">CERRAR SESION</a>
                            <a id="create">Crear producto</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- /Navbar -->

    <!-- Main Content -->
    <div class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">TUS PRODUCTOS</h4>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead class="text-primary">
                                    <tr>
                                        <th>ID</th>
                                        <th>NOMBRE</th>
                                        <th>PRECIO</th>
                                        <th>DESCRIPCION</th>
                                        <th>TALLA</th>
                                        <th>CATEGORIA</th>
                                        <th>ACCIONES</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% datos.forEach(element=> { %>
                                        <tr>
                                            <td>
                                                <%= element.idproducto %>
                                            </td>
                                            <td>
                                                <%= element.nombre %>
                                            </td>
                                            <td>
                                                <%= element.descripcion %>
                                            </td>
                                            <td>
                                                <%= element.precio %>
                                            </td>
                                            <td>
                                                <%= element.talla %>
                                            </td>
                                            <td>
                                                <%= element.categoria %>
                                            </td>
                                            <td>
                                                <a href="#" data-id="<%= element.idproducto %>"
                                                    data-name="<%= element.nombre %>"
                                                    data-desc="<%= element.descripcion %>"
                                                    data-price="<%= element.precio %>" data-link="<%= element.enlace %>"
                                                    data-size="<%= element.size %>"
                                                    data-category="<%= element.category %>"
                                                    class="btn btn-info btn-sm btn-edit">
                                                    <i class="fa fa-pencil-square" aria-hidden="true"></i>
                                                </a>
                                                <a href="#" class="btn btn-danger btn-sm btn-delete"
                                                    data-id="<%= element.idproducto %>">
                                                    <i class="fa fa-trash"></i>
                                                </a>
                                            </td>
                                        </tr>
                                        <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /Main Content -->
